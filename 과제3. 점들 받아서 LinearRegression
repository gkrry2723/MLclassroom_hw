      "source": [
        "#google drive mount 완료\n",
        "from google.colab import drive\n",
        "drive.mount(\"/gdrive\")\n",
        "\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "with open('/gdrive/My Drive/data.csv') as file:\n",
        "  test_data=[]\n",
        "  for line in file.readlines():\n",
        "    test_data.append(line.split(','))\n",
        "\n",
        "#data set 불러오기\n",
        "dataset=np.loadtxt(\"/gdrive/My Drive/data.csv\", delimiter=\",\")\n",
        "print(dataset)\n",
        "#data plot하기\n",
        "length=len(dataset)\n",
        "i=0\n",
        "\n",
        "while(i<length):\n",
        "    plt.scatter(dataset[i][0],dataset[i][1],c=\"black\", s=1)\n",
        "    i=i+1\n",
